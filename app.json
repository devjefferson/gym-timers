{
  "expo": {
    "name": "Temporizador de Treino - Gym Timer",
    "slug": "gym-timers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gymtimers",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#222"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ndiesuper.gymtimers"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#222"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ndiesuper.gymtimers"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8f534c88-5ac0-490e-ad52-356a872954c1"
      }
    }
  }
}
