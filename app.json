{
  "expo": {
    "name": "Temporizador de Treino",
    "slug": "gym-timers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gymtimers",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#222"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 120,
          "imageHeight": 120,
          "resizeMode": "contain",
          "backgroundColor": "#222",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#222"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
